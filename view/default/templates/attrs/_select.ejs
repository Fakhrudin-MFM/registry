<!-- select -->
<div class="form-group <%= field.required?"required":"" %> <%=cssFromTags(field)%>"
     style="<%=styleFromTags(field)%>" data-type="select"
     data-sp="<%= JSON.stringify(prop.meta.selectionProvider) %>"
     data-prop="<%= JSON.stringify(field) %>">
    <label for="<%= id %>" class="col-md-2 col-sm-3 control-label"><%= field.caption %>
        <% include help-sign %>
    </label>
    <div class="col-md-10 col-sm-9">
        <div class="overlay overlay-select"><i class="fa fa-refresh fa-spin"></i></div>
        <select id="<%= id %>" name="<%= prop.getName() %>" class="form-control attr-value" <% if (field.readonly) { %>disabled<% } %>>
            <%
              if (prop.isNullable()) {
                %><option value="" <% if (prop.getValue() === null) {%>selected<%} %>></option><%
              }
            var sel = prop.getSelection();
            if (Array.isArray(sel)) {
              for (var i = 0; i < sel.length; i++) {
            %>
              <option value="<%= sel[i].key %>" <% if (prop.selectionKeyMatch(sel[i].key)) {%>selected<%} %>> <%= sel[i].value %></option>
            <%
              }
            }%>
        </select>
        <p class="error-block"></p>
    </div>
</div>
