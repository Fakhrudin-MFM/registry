<!-- _static-object -->
<div class="form-group <%=cssFromTags(field)%>"
     style="<%=styleFromTags(field)%>" data-type="static-object">
    <label class="col-md-2 col-sm-3 control-label"><%= field.caption %><% include help-sign %></label>
    <div class="col-md-10 col-sm-9">
        <input readonly id="<%= id %>" name="<%= prop.getName() %>"
               class="form-control form-static-value form-object-link" value="<%= prop.getDisplayValue() %>" />
    </div>
</div>
