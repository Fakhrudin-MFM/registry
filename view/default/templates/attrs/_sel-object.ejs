<!-- sel object -->
<div class="form-group <%= field.required?"required":"" %> <%=cssFromTags(field)%>"
     style="<%=styleFromTags(field)%>" data-type="sel-object">
  <label for="<%= id %>" class="col-md-2 col-sm-3 control-label"><%= field.caption %><% include help-sign %></label>
  <div class="col-md-10 col-sm-9">
<div class="form-control-box dropdown-with-tools">
<div class="overlay"><i class="fa fa-refresh fa-spin"></i></div>
<select title='<%= field.caption %>' id="<%= id %>" name="<%= prop.getName() %>" class="form-control attr-value"></select>
</div>
  </div>
</div>
